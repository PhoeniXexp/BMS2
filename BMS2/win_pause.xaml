<Window x:Class="BMS2.win_pause"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BMS2"
        mc:Ignorable="d"
        Title="Pause" Height="90" Width="125" WindowStyle="ToolWindow" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="20" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40
                 "/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Right" Height="20" Margin="0,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40                  "/>
        <Button x:Name="button" Content="OK" HorizontalAlignment="Left" Height="22" Margin="10,37,0,0" VerticalAlignment="Top" Width="99" Click="button_Click"/>

    </Grid>
</Window>
